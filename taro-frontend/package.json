{
  "name": "taro-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "create:page": "gvegas-cfc create -t ./lib/templates/AsyncTemplate -o ./src/pages",
    "create:ui": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/shared/UI/",
    "create:widget": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/widgets/",
    "create:feature": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/features/"
  },
  "dependencies": {
    "freeice": "^2.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "gvegas-cfc": "^3.0.9",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
